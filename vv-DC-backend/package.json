{
  "name": "dental-clinic-backend",
  "version": "1.0.0",
  "type": "module",
  "description": "Backend API for dental clinic management system",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "start:prod": "node src/index.js",
    "dev": "nodemon --watch src --ext js,mjs,cjs,json src/index.js",
    "seed": "node src/seed/seed.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .js",
    "lint:fix": "eslint src --ext .js --fix",
    "format": "prettier --write \"src/**/*.js\""
  },
  "keywords": [
    "dental",
    "clinic",
    "api",
    "mongodb",
    "express",
    "nodejs"
  ],
  "author": "Dental Clinic Team",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.1",
    "express-validator": "^7.2.1",
    "helmet": "^7.1.0",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.6.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.7",
    "swagger-ui-express": "^5.0.0",
    "validator": "^13.15.15",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "eslint": "^8.52.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
